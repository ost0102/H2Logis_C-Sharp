                            <button type="button" class="btns icon delete"><span class="blind">삭제</span></button>
                        </div>
                        <div class="int_box one">
                            <input type="text" id="cbm_width" value="" name="" class="int wgt" placeholder="세로" data-index="LogisticsCBM2" tabindex="1">
                            <button type="button" class="btns icon delete"><span class="blind">삭제</span></button>
                        </div>
                        <div class="int_box one">
                            <input type="text" id="cbm_height" value="" name="" class="int wgt" placeholder="높이" data-index="LogisticsCBM3" tabindex="1">
                            <button type="button" class="btns icon delete"><span class="blind">삭제</span></button>
                        </div>
                        <div class="int_box">
                            <select id="CBM_UNIT">
                                <option value="" selected="">단위</option>
                                <option value="MM">mm</option>
                                <option value="CM">cm</option>
                                <option value="M">m</option>
                                <option value="INCH">inch</option>
                            </select>
